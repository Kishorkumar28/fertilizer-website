    <!-- Background Video Handling -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Get the background video element
            const bgVideo = document.querySelector('.absolute.min-w-full.min-h-full');
            
            if (bgVideo) {
                // Handle video loading errors
                bgVideo.addEventListener('error', function() {
                    console.error('Background video failed to load');
                    // Make sure the fallback background is visible
                    document.querySelector('.absolute.inset-0.bg-gradient-to-r').style.opacity = '1';
                });
                
                // Improve performance by pausing video when not in viewport
                const observer = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            bgVideo.play().catch(e => console.log('Could not autoplay: ', e));
                        } else {
                            bgVideo.pause();
                        }
                    });
                }, { threshold: 0.1 });
                
                observer.observe(bgVideo);
                
                // Handle mobile devices differently - they often block autoplay
                if (/iPhone|iPad|iPod|Android/i.test(navigator.userAgent)) {
                    // Lower resolution/quality for mobile
                    bgVideo.setAttribute('playsinline', '');
                    
                    // Add poster image for better mobile experience
                    bgVideo.setAttribute('poster', 'https://images.unsplash.com/photo-1523348837708-15d4a09cfac2?ixlib=rb-4.0.3&auto=format&fit=crop&w=1470&h=980&q=80');
                    
                    // Try to play on user interaction
                    document.addEventListener('touchstart', () => {
                        bgVideo.play().catch(e => console.log('Could not autoplay on touch: ', e));
                    }, { once: true });
                }
            }
        });
    </script>

